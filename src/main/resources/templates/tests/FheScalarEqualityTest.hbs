@Tag("FheScalarEquality")
@Tag("scalar")
void scalarEquality() {
  logger.trace("scalarEquality");

  {{testSet tested}}
  {{tested.className}} lhsEncrypted = {{tested.className}}.encrypt(lhs, keySet.clientKey());

  FheBool result = lhsEncrypted.scalarEq(rhs);
  Boolean decryptedResult = result.decrypt(keySet.clientKey());
  assertThat(decryptedResult).isEqualTo({{equal tested "lhs" "rhs"}});

  result = lhsEncrypted.scalarNe(rhs);
  decryptedResult = result.decrypt(keySet.clientKey());
  assertThat(decryptedResult).isEqualTo({{notEqual tested "lhs" "rhs"}});
}
