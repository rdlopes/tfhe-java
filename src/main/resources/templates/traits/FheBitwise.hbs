{{#method this (stringFormat "public %s" className) "shiftLeft" "shl" (stringFormat "%s other" className)}}
  {{declare "result"}}
  {{execute "shl" "getValue(), other.getValue(), result.getAddress()"}}
  return result;
{{/method}}

{{#method this (stringFormat "public %s" className) "shiftLeftScalar" "scalar_shl"
          (stringFormat "%s other" valueClassName)}}
  {{declare "result"}}
  {{#if isHighBitSize}}
    {{execute "scalar_shl" "getValue(), other.getAddress(), result.getAddress()"}}
  {{else}}
    {{execute "scalar_shl" "getValue(), other, result.getAddress()"}}
  {{/if}}
  return result;
{{/method}}

{{#method this "public void" "shiftLeftAssign" "shl_assign" (stringFormat "%s other" className)}}
  {{execute "shl_assign" "getValue(), other.getValue()"}}
{{/method}}

{{#method this "public void" "shiftLeftScalarAssign" "scalar_shl_assign" (stringFormat "%s other" valueClassName)}}
  {{#if isHighBitSize}}
    {{execute "scalar_shl_assign" "getValue(), other.getAddress()"}}
  {{else}}
    {{execute "scalar_shl_assign" "getValue(), other"}}
  {{/if}}
{{/method}}

