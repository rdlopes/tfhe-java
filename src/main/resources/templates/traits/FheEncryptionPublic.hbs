{{#method this (stringFormat "public static %s" className) "encrypt" "try_encrypt_with_public_key"
          (stringFormat "%s clearValue" valueClassName) "PublicKey publicKey" override=false}}
  {{declare "encrypted"}}
  {{#if isHighBitSize}}
    {{execute "try_encrypt_with_public_key" "clearValue.getAddress(), publicKey.getValue(), encrypted.getAddress()"}}
  {{else}}
    {{execute "try_encrypt_with_public_key" "clearValue, publicKey.getValue(), encrypted.getAddress()"}}
  {{/if}}
  return encrypted;
{{/method}}
